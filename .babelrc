{
    "plugins": [
        [
            "module-resolver",
            {
                "root": "./dist",
                "extensions": [".ts"],
                "alias": {
                    "@Containers": "./dist/Containers",
                    "@Layouts": "./dist/Fragments",
                    "@Inputs": "./dist/Inputs",
                    "@Text": "./dist/Text",
                    "@Themes": "./dist/Themes",
                    "@Utils": "./dist/Utils"
                }
            }
        ]
    ]
}
